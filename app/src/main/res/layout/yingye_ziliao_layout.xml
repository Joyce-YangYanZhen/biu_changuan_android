<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/yingye_ziliao_layout"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/m20"
            android:layout_marginTop="@dimen/m30"
            android:text="工商信息"
            android:textColor="#ff181818"
            android:textSize="16sp" />


        <LinearLayout
            android:id="@+id/business_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/m10"
            android:background="@color/white"
            android:orientation="vertical"
            android:visibility="visible">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginTop="@dimen/m20"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv133"
                    android:textColor="@color/top_heiziti"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/qiye_mingcheng_name"
                    android:layout_width="@dimen/m265"
                    android:layout_height="@dimen/m31"
                    android:layout_marginLeft="@dimen/m5"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/edit_hint18"
                    android:paddingLeft="@dimen/m20"
                    android:singleLine="true"
                    android:textColor="@color/top_heiziti"
                    android:textColorHint="@color/color_9B9B9B"
                    android:textSize="13sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginTop="@dimen/m25"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="营业执照/统一"
                        android:textColor="@color/top_heiziti"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="社会信用代码"
                        android:textColor="@color/top_heiziti"
                        android:textSize="16sp" />
                </LinearLayout>


                <EditText
                    android:id="@+id/yingyezhizhao_xingyong_edittext"
                    android:layout_width="@dimen/m227"
                    android:layout_height="@dimen/m31"
                    android:layout_marginLeft="@dimen/m4"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/edit_hint34"
                    android:paddingLeft="@dimen/m20"
                    android:singleLine="true"
                    android:textColor="@color/top_heiziti"
                    android:textColorHint="@color/color_9B9B9B"
                    android:textSize="13sp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginTop="@dimen/m20"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv35"
                    android:textColor="@color/top_heiziti"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/faren_name"
                    android:layout_width="@dimen/m265"
                    android:layout_height="@dimen/m31"
                    android:layout_marginLeft="@dimen/m4"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/edit_hint16"
                    android:paddingLeft="@dimen/m20"
                    android:singleLine="true"
                    android:textColor="@color/top_heiziti"
                    android:textColorHint="@color/color_9B9B9B"
                    android:textSize="13sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginTop="@dimen/m20"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv36"
                    android:textColor="@color/top_heiziti"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/icon_id_card"
                    android:layout_width="@dimen/m265"
                    android:layout_height="@dimen/m31"
                    android:layout_marginLeft="@dimen/m4"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/edit_hint17"
                    android:paddingLeft="@dimen/m20"
                    android:singleLine="true"
                    android:textColor="@color/top_heiziti"
                    android:textColorHint="@color/color_9B9B9B"
                    android:textSize="13sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginTop="@dimen/m20"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv135"
                    android:textColor="@color/top_heiziti"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/qiye_zhanghao"
                    android:layout_width="@dimen/m265"
                    android:layout_height="@dimen/m31"
                    android:layout_marginLeft="@dimen/m4"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/edit_hint36"
                    android:paddingLeft="@dimen/m20"
                    android:singleLine="true"
                    android:textColor="@color/top_heiziti"
                    android:textColorHint="@color/color_9B9B9B"
                    android:textSize="13sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginTop="@dimen/m20"
                android:layout_marginBottom="@dimen/m20"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv134"
                    android:textColor="@color/top_heiziti"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/yanzheng_jine"
                    android:layout_width="@dimen/m160"
                    android:layout_height="@dimen/m31"
                    android:layout_marginLeft="@dimen/m4"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/edit_hint35"
                    android:paddingLeft="@dimen/m20"
                    android:singleLine="true"
                    android:textColor="@color/top_heiziti"
                    android:textColorHint="@color/color_9B9B9B"
                    android:textSize="13sp" />

                <LinearLayout
                    android:layout_width="@dimen/m95"
                    android:layout_height="@dimen/m28"
                    android:layout_marginLeft="@dimen/m10"
                    android:background="@drawable/yanzheng_btn_bg"
                    android:gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="发送验证金额"
                        android:textColor="#ff181818"
                        android:textSize="13sp" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/m20"
            android:layout_marginTop="@dimen/m10"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/m17"
                android:layout_height="@dimen/m17"
                android:src="@drawable/gantanhao" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/m10"
                android:text="@string/edit_hint20"
                android:textColor="@color/color_9B9B9B"
                android:textSize="13sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/submit_btn"
            android:layout_width="@dimen/m209"
            android:layout_height="@dimen/m50"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/m30"
            android:background="@drawable/tixian_btn"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/tv136"
                android:textColor="@color/top_heiziti"
                android:textSize="20sp" />

        </LinearLayout>
    </LinearLayout>

</LinearLayout>