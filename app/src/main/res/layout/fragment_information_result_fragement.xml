<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".fragment.InformationResultFragement">

    <ImageView
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginTop="@dimen/m119"
        android:src="@drawable/shenhe_icon"
        android:layout_gravity="center_horizontal"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tv40"
        android:layout_marginTop="@dimen/m9"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/top_heiziti"
        android:textSize="@dimen/sp20"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tv41"
        android:layout_marginTop="@dimen/m13"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/color_4A4A4A"
        android:textSize="@dimen/sp15"
        />


</LinearLayout>